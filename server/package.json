{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build": "babel --delete-dir-on-start --out-dir dist --copy-files --ignore \"**/__tests__/**,**/__mocks__/**\" --no-copy-ignored src",
    "start:dev": "nodemon dist/index.js",
    "build:dev": "tsc --watch --preserveWatchOutput",
    "dev": "concurrently \"npm:build:dev\" \"npm:start:dev\"",
    "test": "mocha test/index.js"
  },
  "dependencies": {
    "@types/xml2js": "^0.4.9",
    "axios": "^0.21.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "node-html-parser": "^4.1.3",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.0.0",
    "chai": "^4.3.4",
    "concurrently": "^6.2.1",
    "mocha": "^9.1.0",
    "supertest": "^6.1.6",
    "typescript": "^4.3.5"
  }
}
